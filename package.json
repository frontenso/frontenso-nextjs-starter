{
  "name": "frontenso-nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "next dev",
    "build": "npm run validate:ci && next build && next export",
    "format": "pretty-quick --staged",
    "lint:css": "stylelint \"./**/*.{jsx,js}\"",
    "lint:js": "eslint --max-warnings 0 \"./**/*.{jsx,js}\"",
    "validate": "npm-run-all --parallel format lint:css lint:js",
    "validate:ci": "npm-run-all --parallel lint:css lint:js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "dependencies": {
    "@linaria/babel-preset": "^3.0.0-beta.1",
    "@linaria/core": "^3.0.0-beta.1",
    "@linaria/react": "^3.0.0-beta.1",
    "@linaria/shaker": "^3.0.0-beta.1",
    "@linaria/stylelint": "^3.0.0-beta.1",
    "@linaria/webpack-loader": "^3.0.0-beta.1",
    "@zeit/next-css": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "next": "10.0.5",
    "next-images": "^1.7.0",
    "next-pwa": "^5.0.5",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-svg-loader": "^3.0.3",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended": "^3.0.0"
  }
}
